int main()
int n;
int first;
int second;
int next;
int c;
{ 
   output "Enter the number of terms: \n";
   input n;
 
   output "First ",n," terms of Fibonacci series are : -\n";
 
  c=0;
  while(c<n)do{
    if ( c <= 1 )
         next = c
    else
      {
         next = first + second;
         first = second;
         second = next;
      };
     output next,"\n";
     c=c+1
  };
 
   return 0;
}
